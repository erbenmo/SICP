(define (A x y)
  (cond ((= y 0) 0)
	((= x 0) (* 2 y))
	((= y 1) 2)
	(else (A (- x 1)
		 (A x (- y 1))))))

;;; (A 1 X) = 2^X

;;; (A 2 4) = 2^(A 2 3)
;;; (A 2 3) = 2^(A 2 2)
;;; (A 2 2) = 2^(A 2 1)
;;; (A 2 1) = 2
;;; (A 2 X) = 2^(2^(2^2^2^

;;; (A 3 3) = (A 2 (A 3 2))
;;; (A 3 2) = (A 2 (A 3 1))
;;; (A 3 1) = 2


(define (f n) (A 0 n))
;;; f = 2*n

(define (g n) (A 1 n))
;;; g = 2^n

(define (h n) (A 2 n))
;;; h = 2^2^2^ --- 2^0 (n's 2)